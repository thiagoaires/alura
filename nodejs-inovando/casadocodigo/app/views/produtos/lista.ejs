<html>

<body>
    <h1>lista produtos</h1>
    <table>
        <tr>
            <td>ID</td>
            <td>Nome</td>
            <td>Descrição</td>
            <td>Preço</td>
        </tr>
        <% for(var i = 0; i < lista.length ; i++){ %>
            <tr>
                <td>
                    <%=lista[i].id%>
                </td>
                <td>
                    <%=lista[i].titulo%>
                </td>
                <td>
                    <%=lista[i].descricao%>
                </td>
                <td>
                    <%=lista[i].preco%>
                </td>
            </tr>
            <%}%>
    </table>
</body>

</html>